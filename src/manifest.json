{
    "name": "Mommy-Please",
    "version": "0.1",
    "manifest_version": 2,
    "description": "A Chromium Extension to ask mommy for a website.",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "background": {
        "scripts": ["background.js"]
      },
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "options_page": "options/options.html",
    "permissions": [
        "storage",
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
      ]
}