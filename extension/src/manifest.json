{
  "name": "Mommy-Please",
  "version": "0.5.0",
  "manifest_version": 2,
  "description": "A Chromium Extension to ask mommy for a website.",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "page": "background/background.html"
  },
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "options_page": "options/options.html",
  "permissions": ["storage", "tabs", "http://*/*", "https://*/*"]
}
